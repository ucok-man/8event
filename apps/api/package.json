    "ioredis": "^5.5.0",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.4.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "pug": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "uuid": "^11.0.5",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
