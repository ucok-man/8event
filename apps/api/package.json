    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.41.5",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "copy-folder-util": "^1.1.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
